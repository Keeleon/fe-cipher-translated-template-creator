{
  "extends": "tslint-config-airbnb",
  "rules": {
    "trailing-comma": false,
    "align": false,
    "max-line-length": [
      true,
      120
    ],
    "no-trailing-whitespace": true,
    "no-increment-decrement": false,
    "no-unused-variable": true
  },
  "linterOptions": {
    "exclude": [
      "/**/*.json",
      "/**/*.csv",
      "/**/*.xml",
      "/**/*.xls",
      "/**/*.xlsx"
    ]
  }
}