{
  "name": "fe-cipher-translated-template-creator",
  "version": "0.0.1",
  "description": "Scrapes the Fire Emblem CCG Wiki for card descriptions and outputs them into a printable format.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "postinstall": "npm run compile",
    "prepublishOnly": "npm run compile",
    "compile": "tsc -p . --declaration",
    "prestart": "npm run compile",
    "start": "node dist/index.js",
    "watch": "tsc --watch",
    "unit-test": "mocha test/**/*.spec.* --recursive --compilers ts:ts-node/register",
    "pretest": "npm run lint",
    "test": "nyc npm run unit-test",
    "posttest": "npm run mutate",
    "mutate": "stryker run",
    "lint": "tslint --project ."
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^2.2.46",
    "@types/sinon": "^4.1.3",
    "app-root-path": "^2.0.1",
    "chai": "^4.1.2",
    "mocha": "3.4.2",
    "mocha-typescript": "^1.1.12",
    "nyc": "^11.4.1",
    "sinon": "^4.3.0",
    "stryker": "^0.17.0",
    "stryker-api": "^0.12.0",
    "stryker-html-reporter": "^0.11.4",
    "stryker-mocha-framework": "^0.7.2",
    "stryker-mocha-runner": "^0.10.5",
    "stryker-typescript": "^0.8.3",
    "ts-node": "^4.1.0",
    "tsc": "1.20150623.0",
    "tslint": "5.9.1",
    "tslint-config-airbnb": "5.4.2",
    "typescript": "2.5.3"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.7",
    "@types/jsdom": "^11.0.4",
    "@types/log4js": "^2.3.5",
    "@types/node": "^9.4.7",
    "@types/q": "^1.5.0",
    "@types/request": "^2.47.0",
    "html-to-text": "^4.0.0",
    "jsdom": "^11.8.0",
    "log4js": "^2.3.10",
    "q": "^1.5.1",
    "request": "^2.85.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
